
config dnsproxy 'global'
	option enabled '1'
	list listen_addr '127.0.0.1'
	list listen_addr '::1'
	list listen_port '5350'
	option log_file ''
	option all_servers '0'
	option fastest_addr '0'
	option http3 '0'
	option insecure '0'
	option ipv6_disabled '0'
	option timeout ''
	option max_go_routines ''
	option rate_limit ''
	option refuse_any '1'
	option udp_buf_size ''
	option verbose '0'

config dnsproxy 'bogus_nxdomain'
	list ip_addr ''

config dnsproxy 'cache'
	option enabled '1'
	option cache_optimistic '1'
	option size '900000'
	option min_ttl '3600'
	option max_ttl '86400'

config dnsproxy 'dns64'
	option enabled '0'
	option dns64_prefix '64:ff9b::'

config dnsproxy 'edns'
	option enabled '0'
	option edns_addr ''

config dnsproxy 'private_rdns'
	option enabled '0'
	list upstream '127.0.0.1:53'

config dnsproxy 'servers'
	list bootstrap '223.6.6.6'
	list upstream 'https://dns.alidns.com/dns-query'

config dnsproxy 'tls'
	option enabled '0'
	option tls_crt ''
	option tls_key ''
	option https_port '8443'
	option tls_port '853'
	option quic_port '853'
